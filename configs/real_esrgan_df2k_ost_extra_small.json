{
    "name": "real_esrgan_df2k_ost_extra_small",
    "dataset": "df2k_ost_small",
    "batch_size": 64,
    "shuffle": true,
    "epochs": 50,
    "early_stopping": true,
    "early_stopping_monitor": "train_loss",
    "early_stopping_patience": 5,
    "early_stopping_min_delta": -0.002,
    "early_stopping_min_delta_strategy": "previous_proportional",
    "save_after_n_epoch": true,
    "save_after_n_epoch_period": 1,
    "show_plot": true,
    "save_plot": true,
    "model": "real_esrgan",
    "model_params": {
        "beta": 0.2,
        "rrdb_layers": 16,
        "image_size": 48
    },
    "g_optimizer": "adamw",
    "g_optimizer_params": {
        "param_type": "adamw",
        "lr": 0.0001,
        "betas": [
            0.9,
            0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0.01,
        "amsgrad": false,
        "maximize": false,
        "capturable": false,
        "differentiable": false
    },
    "d_optimizer": "adamw",
    "d_optimizer_params": {
        "param_type": "adamw",
        "lr": 5e-05,
        "betas": [
            0.9,
            0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0.01,
        "amsgrad": false,
        "maximize": false,
        "capturable": false,
        "differentiable": false
    },
    "g_loss_function": "srgan_generator_loss",
    "d_loss_function": "esrgan_discriminator_loss",
    "fake_label": 0.0,
    "real_label": 0.95,
    "pretrain": true,
    "pretrain_epochs": 5,
    "pretrain_batch_size": 32,
    "pretrain_optimizer": "adamw",
    "pretrain_optimizer_params": {
        "param_type": "adamw",
        "lr": 0.0001,
        "betas": [
            0.9,
            0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0.01,
        "amsgrad": false,
        "maximize": false,
        "capturable": false,
        "differentiable": false
    },
    "pretrain_loss_function": "l1",
    "save_pretrained": true,
    "load_pretrained": true,
    "show_pretrained_plot": false,
    "save_pretrained_plot": true
}