{
    "name": "vae_mnist",
    "dataset": "mnist",
    "batch_size": 128,
    "shuffle": true,
    "epochs": 100,
    "early_stopping": true,
    "early_stopping_monitor": "train_loss",
    "early_stopping_patience": 5,
    "early_stopping_min_delta": -0.002,
    "early_stopping_min_delta_strategy": "previous_proportional",
    "save_after_n_epoch": true,
    "save_after_n_epoch_period": 10,
    "show_plot": true,
    "save_plot": true,
    "model": "vae",
    "model_params": {},
    "optimizer": "adam",
    "optimizer_params": {
        "param_type": "adamw",
        "lr": 0.0003,
        "betas": [
            0.9,
            0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0.01,
        "amsgrad": false,
        "maximize": false,
        "capturable": false,
        "differentiable": false
    },
    "loss_function": "vae_loss"
}