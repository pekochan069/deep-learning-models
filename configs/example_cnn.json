{
    "name": "example_cnn",
    "dataset": "mnist",
    "batch_size": 64,
    "shuffle": false,
    "epochs": 15,
    "early_stopping": false,
    "early_stopping_monitor": "val_loss",
    "early_stopping_patience": 5,
    "early_stopping_min_delta": 0.0,
    "early_stopping_min_delta_strategy": "fixed",
    "save_after_n_epoch": false,
    "save_after_n_epoch_period": 1,
    "show_plot": true,
    "save_plot": true,
    "model": "example_cnn",
    "model_params": {},
    "optimizer": "adam",
    "optimizer_params": {
        "param_type": "adam",
        "lr": 0.001,
        "betas": [
            0.9,
            0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0,
        "amsgrad": false,
        "maximize": false,
        "capturable": false,
        "differentiable": false,
        "decoupled_weight_decay": false
    },
    "loss_function": "cross_entropy"
}