{
    "name": "res_net50_cifar10",
    "model": "res_net50",
    "dataset": "cifar10",
    "batch_size": 64,
    "shuffle": true,
    "optimizer": "adam",
    "optimizer_params": {
        "lr": 0.001,
        "weight_decay": 0.0001
    },
    "loss_function": "cross_entropy",
    "epochs": 30,
    "early_stopping": true,
    "early_stopping_monitor": "val_loss",
    "early_stopping_patience": 5,
    "early_stopping_min_delta": 0.05,
    "model_params": {}
}